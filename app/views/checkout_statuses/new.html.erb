<h1>New checkout_status</h1>

<% form_for(@checkout_status) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :video_id %><br />
    <%= f.text_field :video_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', checkout_statuses_path %>